{
  "name": "bank-demo",
  "implicitDependencies": ["sdk"],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/bank-demo",
        "command": "PORT=3005 nuxt dev"
      },
      "dependsOn": ["^build"]
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/bank-demo",
        "command": "nuxt generate"
      },
      "dependsOn": ["^build"]
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/bank-demo",
        "command": "nuxt preview"
      },
      "dependsOn": ["^build", "build"]
    }
  }
}
