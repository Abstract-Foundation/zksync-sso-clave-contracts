{
  "private": true,
  "type": "module",
  "description": "ZKsync Smart Account SDK",
  "version": "0.0.0-development",
  "publishConfig": {
    "registry": "http://localhost:4873"
  },
  "license": "MIT",
  "author": "Matter Labs",
  "repository": {
    "type": "git",
    "url": "https://github.com/matter-labs/zksync-account-sdk.git"
  },
  "scripts": {
    "publish:local": "pnpm publish --no-git-checks --force"
  },
  "peerDependencies": {
    "@simplewebauthn/browser": "10.x",
    "@simplewebauthn/server": "10.x",
    "@wagmi/core": "2.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@simplewebauthn/types": "^10.0.0",
    "@types/node": "^22.1.0",
    "eventemitter3": "^5.0.1",
    "viem": "^2.21.14"
  },
  "files": [
    "*",
    "!**/*.test.ts",
    "!**/*.test-d.ts",
    "!**/*.tsbuildinfo",
    "!tsconfig.build.json"
  ],
  "main": "./_cjs/index.js",
  "module": "./_esm/index.js",
  "types": "./_types/index.d.ts",
  "typings": "./_types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./_types/index.d.ts",
      "import": "./_esm/index.js",
      "require": "./_cjs/index.js"
    },
    "./connector": {
      "types": "./_types/connector/index.d.ts",
      "import": "./_esm/connector/index.js",
      "require": "./_cjs/connector/index.js"
    },
    "./client-gateway": {
      "types": "./_types/client-gateway/index.d.ts",
      "import": "./_esm/client-gateway/index.js",
      "require": "./_cjs/client-gateway/index.js"
    },
    "./client": {
      "types": "./_types/client/index.d.ts",
      "import": "./_esm/client/index.js",
      "require": "./_cjs/client/index.js"
    },
    "./client/passkey": {
      "types": "./_types/client/passkey.d.ts",
      "import": "./_esm/client/passkey.js",
      "require": "./_cjs/client/passkey.js"
    },
    "./client/smart-account": {
      "types": "./_types/client/smart-account.d.ts",
      "import": "./_esm/client/smart-account.js",
      "require": "./_cjs/client/smart-account.js"
    },
    "./client/actions": {
      "types": "./_types/client/actions/index.d.ts",
      "import": "./_esm/client/actions/index.js",
      "require": "./_cjs/client/actions/index.js"
    },
    "./client/decorators": {
      "types": "./_types/client/decorators/index.d.ts",
      "import": "./_esm/client/decorators/index.js",
      "require": "./_cjs/client/decorators/index.js"
    },
    "./communicator": {
      "types": "./_types/communicator/index.d.ts",
      "import": "./_esm/communicator/index.js",
      "require": "./_cjs/communicator/index.js"
    },
    "./errors": {
      "types": "./_types/errors/index.d.ts",
      "import": "./_esm/errors/index.js",
      "require": "./_cjs/errors/index.js"
    },
    "./utils": {
      "types": "./_types/utils/index.d.ts",
      "import": "./_esm/utils/index.js",
      "require": "./_cjs/utils/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./_types/index.d.ts"
      ],
      "client": [
        "./_types/client/index.d.ts"
      ],
      "client-gateway": [
        "./_types/client-gateway/index.d.ts"
      ],
      "communicator": [
        "./_types/communicator/index.d.ts"
      ],
      "connector": [
        "./_types/connector/index.d.ts"
      ],
      "errors": [
        "./_types/errors/index.d.ts"
      ]
    }
  },
  "keywords": [
    "zksync",
    "smart",
    "account",
    "eth",
    "ethereum",
    "wallet",
    "web3"
  ],
  "dependencies": {
    "@peculiar/asn1-ecc": "^2.3.13",
    "@peculiar/asn1-schema": "^2.3.13",
    "abitype": "^1.0.6",
    "bigint-conversion": "^2.4.3",
    "buffer": "^6.0.3",
    "cbor-web": "^9.0.2"
  }
}
